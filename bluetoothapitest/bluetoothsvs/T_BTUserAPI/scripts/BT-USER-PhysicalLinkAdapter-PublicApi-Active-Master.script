START_TESTCASE BT-USER-Authenticate-PublicApi-Active-Master-0001
//! @SYMTestCaseID				BT-USER-Authenticate-PublicApi-Active-Master-0001
//! @SYMAPI						RSocketServ, CBluetoothPhysicalLinks, MBluetoothPhysicalLinksNotifier, RBTPhysicalLinkAdapter, CBluetoothSocket, MBluetoothSocketNotifier
//! @SYMTestCaseDesc			Attempts to authenticate the existing physical link
//!								Uses API elements: 	RSocketServ() and Connect() from RSocketServ
//!													NewL() and CreateConnection() from CBluetoothPhysicalLinks
//!													HandleCreateConnectionCompleteL() from MBluetoothPhysicalLinksNotifier
//!													RBTPhysicalLinkAdapter(), Open()2, NotifyNextBasebandChangeEvent(), Authenticate(), Close() and destructor from RBTPhysicalLinkAdapter
//!													NewL()1 and Connect()1 from CBluetoothSocket
//!													HandleConnectCompleteL() from MBluetoothSocketNotifier
//! @SYMTestActions				1. Create a connection to the RSocket server
//!								2. Create the CBluetoothSocket object using NewL()1 (Standard constructor)
//!								3. Use the TL2CAPSockAddr in the connect of the CBluetooth object
//!								4. HandleConnectCompleteL gets called indicating that connecting the BT connection completed
//!								5. Create and start a session on the socket server
//!								6. Open a Bluetooth socket using NewL() function. Return KErrNone if successful
//!								7. Bring in a new member to the piconet with a slave device address using CreateConnection()
//!								   function. Return KErrNone if successful
//!								8. Return callback HandleCreateConnectionCompleteL() function
//!								9. Verify that if return result (decimal 0) from callback function is as expected (decimal 0)
//!								10. Create a RBTPhysicalLinkAdapter object using RBTPhysicalLinkAdapter() function
//!								11. Open a physical link adapter on an existing physical link with a device address
//!								    using Open()2 function. Return KErrNone if successful
//!								12. Request a notification with ENotifyAuthenticationComplete using NotifyNextBasebandChangeEvent function
//!								13. Verify that the result (262144) is as expected (262144)
//!								14. Attempts to authenticate the existing physical link using Authenticate() function. 
//!								    Return KErrNone if successful
//!								15. Close the physical link adapter using Close() function
//!								16. Destroy the physical link adapter object
//!
//! @SYMTestStatus				Implemented
//! @SYMTestPriority			Critical
//! @SYMTestExpectedResults		KErrNone returned
//! @SYMTestType				CIT

	START_TEST_BLOCK	500	T_BTUserAPI	\bluetooth\user\BT-USER-PhysicalLinkAdapter-PublicApi-Active-Master.ini
			SHARED_ACTIVE_SCHEDULER
			CREATE_OBJECT	CBluetoothSocket	CBluetoothSocketData	
			COMMAND		CBluetoothSocketData	RSocketServerConnect		
			COMMAND		CBluetoothSocketData	NewL		BT-USER-Authenticate-PublicApi-Active-0001-0001command2NewL
			STORE	CBluetoothSocketData	
			STORE_ACTIVE_SCHEDULER
	END_TEST_BLOCK

	RUN_TEST_STEP -1 UCCControlTE RunCommand \bluetooth\user\UccControlChannelConfig.ini StartSyncTestCase
DELAY		10000
	START_TEST_BLOCK	500	T_BTUserAPI	\bluetooth\user\BT-USER-PhysicalLinkAdapter-PublicApi-Active-Master.ini
			SHARED_ACTIVE_SCHEDULER
			RESTORE_OBJECT	CBluetoothSocket	CBluetoothSocketData	
			RESTORE_OBJECT	TL2CAPSockAddr	TL2CAPSockAddr1	
			COMMAND		CBluetoothSocketData	Connect		BT-USER-Authenticate-PublicApi-Active-0001-0002command2Connect
			OUTSTANDING
			STORE	CBluetoothSocketData	
			STORE	TL2CAPSockAddr1	
			STORE_ACTIVE_SCHEDULER
	END_TEST_BLOCK
	RUN_TEST_STEP -1 UCCControlTE RunCommand \bluetooth\user\UccControlChannelConfig.ini RetrieveSyncTestCaseResult

	RUN_TEST_STEP -1 UCCControlTE RunCommand \bluetooth\user\UccControlChannelConfig.ini StartSyncTestCase
	START_TEST_BLOCK	500	T_BTUserAPI	\bluetooth\user\BT-USER-PhysicalLinkAdapter-PublicApi-Active-Master.ini
			SHARED_ACTIVE_SCHEDULER
			CREATE_OBJECT	RBTPhysicalLinkAdapter	RBTPhysicalLinkAdapterData	
			CREATE_OBJECT	CBluetoothPhysicalLinks	CBluetoothPhysicalLinksData	
			RESTORE_OBJECT	TBTDevAddr	TBTDevAddrRemote	
			CREATE_OBJECT	RSocketServ	RSocketServData	
			COMMAND		RSocketServData	RSocketServ		
			COMMAND		RSocketServData	Connect		
			COMMAND		CBluetoothPhysicalLinksData	NewL		BT-USER-Authenticate-PublicApi-Active-0001-0003command5NewL
			COMMAND		CBluetoothPhysicalLinksData	CreateConnection		BT-USER-Authenticate-PublicApi-Active-0001-0003command6CreateConnection
			OUTSTANDING	
			COMMAND		RBTPhysicalLinkAdapterData	RBTPhysicalLinkAdapter		
			COMMAND		RBTPhysicalLinkAdapterData	Open		BT-USER-Authenticate-PublicApi-Active-0001-0003command10Open
			COMMAND		RBTPhysicalLinkAdapterData	NotifyNextBasebandChangeEvent		BT-USER-Authenticate-PublicApi-Active-0001-0003command11NotifyNextBasebandChangeEvent
			COMMAND		RBTPhysicalLinkAdapterData	Authenticate		
			OUTSTANDING	
			COMMAND		RBTPhysicalLinkAdapterData	Close		
			COMMAND		RBTPhysicalLinkAdapterData	~		
			STORE	TBTDevAddrRemote	
			STORE	RSocketServData	
			STORE	CBluetoothPhysicalLinksData	
			STORE_ACTIVE_SCHEDULER
	END_TEST_BLOCK
	RUN_TEST_STEP -1 UCCControlTE RunCommand \bluetooth\user\UccControlChannelConfig.ini RetrieveSyncTestCaseResult

END_TESTCASE BT-USER-Authenticate-PublicApi-Active-Master-0001


START_TESTCASE BT-USER-Authenticate-PublicApi-Active-Master-0002
//! @SYMTestCaseID				BT-USER-Authenticate-PublicApi-Active-Master-0002
//! @SYMAPI						RSocketServ, CBluetoothPhysicalLinks, RBTPhysicalLinkAdapter, CBluetoothSocket, MBluetoothSocketNotifier
//! @SYMTestCaseDesc			Attempts to authenticate the existing physical link
//!								Uses API elements: 	RBTPhysicalLinkAdapter(), Open()2, Authenticate(), Close() and destructor from RBTPhysicalLinkAdapter
//!													destructor from CBluetoothPhysicalLinks
//!													Close() from RSocketServ
//!													Shutdown()1 and destructor from CBluetoothSocket
//!													HandleShutdownCompleteL() from MBluetoothSocketNotifier
//! @SYMTestActions				1. Create a RBTPhysicalLinkAdapter object using RBTPhysicalLinkAdapter() function
//!								2. Open a physical link adapter on an existing physical link with a device address
//!								   using Open()2 function. Return KErrNone if successful
//!								3. Attempts to authenticate the existing physical link using Authenticate() function. 
//!								   Return KErrNone if successful
//!								4. Close the physical link adapter using Close() function
//!								5. Destroy the physical link adapter object
//!								6. Delete CBluetoothPhysicalLink object
//!								7. Close RSocket object
//!								8. Do socket cleanup by calling Shutdown()1
//!								9. HandleShutdownCompleteL gets called verify error code
//!								10. Delete CBluetoothSocket object
//!
//! @SYMTestStatus				Implemented
//! @SYMTestPriority			Critical
//! @SYMTestExpectedResults		Error at step 3. KErrAlreadyExist (-11) returned
//! @SYMTestType				CIT

	RUN_TEST_STEP -1 UCCControlTE RunCommand \bluetooth\user\UccControlChannelConfig.ini StartSyncTestCase
	START_TEST_BLOCK	500	T_BTUserAPI	\bluetooth\user\BT-USER-PhysicalLinkAdapter-PublicApi-Active-Master.ini
			SHARED_ACTIVE_SCHEDULER
			CREATE_OBJECT	RBTPhysicalLinkAdapter	RBTPhysicalLinkAdapterData	
			RESTORE_OBJECT	TBTDevAddr	TBTDevAddrRemote	
			RESTORE_OBJECT	RSocketServ	RSocketServData	
			COMMAND		RBTPhysicalLinkAdapterData	RBTPhysicalLinkAdapter		
			COMMAND		RBTPhysicalLinkAdapterData	Open		BT-USER-Authenticate-PublicApi-Active-0002-0001command3Open
			COMMAND !Error=-11		RBTPhysicalLinkAdapterData	Authenticate		
			COMMAND		RBTPhysicalLinkAdapterData	Close		
			COMMAND		RBTPhysicalLinkAdapterData	~		
			STORE	RSocketServData	
			STORE	TBTDevAddrRemote	
			STORE_ACTIVE_SCHEDULER
	END_TEST_BLOCK
	RUN_TEST_STEP -1 UCCControlTE RunCommand \bluetooth\user\UccControlChannelConfig.ini RetrieveSyncTestCaseResult

	RUN_TEST_STEP -1 UCCControlTE RunCommand \bluetooth\user\UccControlChannelConfig.ini StartSyncTestCase
DELAY 5000
	START_TEST_BLOCK	500	T_BTUserAPI	\bluetooth\user\BT-USER-PhysicalLinkAdapter-PublicApi-Active-Master.ini
			SHARED_ACTIVE_SCHEDULER
			RESTORE_OBJECT	CBluetoothPhysicalLinks	CBluetoothPhysicalLinksData	
			RESTORE_OBJECT	CBluetoothSocket	CBluetoothSocketData	
			RESTORE_OBJECT	RSocketServ	RSocketServData	
			COMMAND		CBluetoothPhysicalLinksData	~		
			COMMAND		RSocketServData	Close		
			COMMAND		CBluetoothSocketData	Shutdown		BT-USER-Authenticate-PublicApi-Active-0002-0002command6Shutdown
			OUTSTANDING	
			COMMAND		CBluetoothSocketData	~		
			COMMAND		CBluetoothSocketData	RSocketServerClose		
	END_TEST_BLOCK
	RUN_TEST_STEP -1 UCCControlTE RunCommand \bluetooth\user\UccControlChannelConfig.ini RetrieveSyncTestCaseResult

END_TESTCASE BT-USER-Authenticate-PublicApi-Active-Master-0002


