 // backup the commdb settings to c:\cedout.cfg
RUN_WS_PROGRAM -1 ceddump

// apply the settings for this test
CED z:\testdata\btromconfig\pan001.cfg

LOAD_SERVER TestServerSymbianExcludeBt

START_TESTCASE BT-ROMCONFIG-PAN-001
//! @SYMTestCaseID BT-ROMCONFIG-PAN-001
//! @SYMTestCaseDesc Cannot start a BT PAN RConnection
//! on a !__BT ROM
//! @SYMREQ 5413
//! @SYMTestType CIT
//! @SYMTestPriority 1
//! @SYMTestStatus Implemented
//! @SYMTestActions Setup CommDB to make the BT PAN the default IAP. Call RConnection::Start()
//! @SYMTestExpectedResults Returns KErrNotFound as PAN profile is not included on ROM
RUN_TEST_STEP 30 TestServerSymbianExcludeBt BT_ROMCONFIG_PAN_001 c:\testdata\btromconfig\BtRomConfig.ini BluetoothExcluded
END_TESTCASE BT-ROMCONFIG-PAN-001

CED c:\cedout.cfg // restore the original settings

